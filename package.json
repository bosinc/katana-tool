{
  "name": "katana-tool",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm version minor -m 'chore: version to %s' && tsc && vite build",
    "build:staging": "pnpm version patch -m 'chore: version to %s' && tsc && vite build --mode staging",
    "build:release": "pnpm version patch -m 'chore: version to %s' && tsc && vite build --mode release",
    "build:local": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "sync-common": "git submodule update --init",
    "lastest-common": "git submodule foreach git pull origin main"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "ahooks": "^3.7.11",
    "axios": "^1.6.8",
    "blueimp-md5": "^2.19.0",
    "concurrently": "^8.2.2",
    "currency.js": "^2.0.4",
    "jotai": "^2.8.0",
    "lz-string": "^1.5.0",
    "notistack": "^3.0.1",
    "prettier": "^3.2.5",
    "ramda": "^0.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.23",
    "@types/blueimp-md5": "^2.18.2",
    "@types/chrome": "^0.0.266",
    "@types/node": "^20.12.8",
    "@types/ramda": "^0.29.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
